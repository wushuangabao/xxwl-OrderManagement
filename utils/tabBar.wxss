.tab-bar {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  height: 120rpx;
  border-top: 1rpx solid gray;
  background: #F8F8F8;
}

.menu-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  height: 100%;
}

.tab-img {
  width: 61rpx;
  height: 61rpx;
}
