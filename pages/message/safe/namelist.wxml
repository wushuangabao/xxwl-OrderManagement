<!--pages/message/safe/namelist.wxml-->
<view style="display:flex;" catchtap="addAmount">
  <l-icon name="add" color='#34BFA3'> </l-icon>
  <view>增添社保人员</view>
</view>
<view wx:for="{{listData}}" wx:key="" class="list" bindtap="showMoreInfo" data-id="{{item.index}}">
  <view class="list-item">
    <view class='horizontal'>
      <text>{{item.name}}</text>
      <view catchtap="deleteAmount" data-id="{{item.index}}">删除</view>
    </view>
    <view class='horizontal'>
      <text>社保号</text>
      <text>{{item.safe_id}}</text>
    </view>
    <view class='horizontal'>
      <text>企业缴纳</text>
      <text>{{item.firm_amount}}</text>
    </view>
    <view class='horizontal'>
      <text>个人缴纳</text>
      <text>{{item.person_amount}}</text>
    </view>
    <view class='horizontal'>
      <text>总计</text>
      <text>{{item.safeAmount}}</text>
    </view>
  </view>
</view>